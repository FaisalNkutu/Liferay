AUI().ready("aui-dialog","node",function(a){a.one("h3#type-point-role-title").on("click",function(){a.one("div#type-point-role-slide").slideToggle("slow")});a.all("form#<portlet:namespace />fm :text").on("blur",function(){var e=this.size();var c=0;for(c=0;c<e;c++){var b="#<portlet:namespace />point"+c;var h=a.one(b);var f=h.val();var d=0;for(d=0;d<f.length;d++){if(!((f[d]>="0"&&f[d]<="9")||f[d]==" ")){alert(f+' <liferay-ui:message key="invalid" />');h.val("");return false}}var g=f.split(" ");for(d=0;d<g.length;d++){if(g[d]<0||g[d]>10){alert(parseInt(g[d])+' <liferay-ui:message key="point" /> <liferay-ui:message key="invalid" />');h.val("");return false}}}return true})});