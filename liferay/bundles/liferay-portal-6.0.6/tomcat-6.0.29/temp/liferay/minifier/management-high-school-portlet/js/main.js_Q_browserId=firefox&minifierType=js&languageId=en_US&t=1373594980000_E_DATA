AUI().ready("aui-dialog","node",function(a){a.one("h3#type-point-role-title").on("click",function(){a.one("div#type-point-role-slide").slideToggle("slow")});a.one("h3#manager-class-role-title").on("click",function(){a.one("div#manager-class-role-slide").slideToggle("slow")});a.one("h3#manager-class-role-title").on("click",function(){var b=a.all("h3#manager-class-role-title div#lass-group-id").slideToggle("slow");b.each(function(d,c){this.on("click",function(){this.slideToggle("slow")})})})});